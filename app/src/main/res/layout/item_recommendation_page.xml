                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginRight="15dp"
                android:scaleType="centerInside"
                tools:src="@drawable/store" />
            <ImageView
                android:id="@+id/card_comment"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:scaleType="centerInside"
                tools:src="@drawable/comment" />
        </LinearLayout>

    </RelativeLayout>
